{
    "family": "server-task",
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "3072",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "executionRoleArn": "arn:aws:iam::891377404305:role/ecsTaskExecutionRole",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
        {
            "name": "server-container",
            "image": "891377404305.dkr.ecr.ap-northeast-2.amazonaws.com/here-trillionaires/try-and-review/server",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 19090,
                    "hostPort": 19090,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/server-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "volumes": [],
    "placementConstraints": [],
    "tags": []
}
